{
  "name": "nanomonx/hashids-bundle",
  "type": "symfony-bundle",
  "description": "Integrates hashids/hashids in a Symfony project. Adapt roukmoute/hashids-bundle to makes it working with php8 and symfony 5.*",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Quentin Debest",
      "email": "quentin.debest@nanomonx.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Roukmoute\\HashidsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Roukmoute\\HashidsBundle\\": "spec/"
    }
  },
  "require": {
    "php": ">=7.1 | 8.*",
    "hashids/hashids": "^4.1",
    "phpspec/phpspec" : "^7.0",
    "sensio/framework-extra-bundle": "^6",
    "symfony/http-kernel": "^5.3"
  },
  "require-dev": {
    "twig/twig": "^3.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.3"
    }
  },
  "suggest": {
    "twig/twig": "Allows to use hashids in Twig template engine"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
